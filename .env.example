# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/rag_demo
MONGODB_DB=rag_demo
MONGODB_COLLECTION=documents

# Vector Store Configuration
VECTORSTORE=ATLAS
FAISS_INDEX_DIR=.faiss_index

# Embeddings Configuration
EMBEDDINGS=OPENAI
HF_EMBEDDINGS_MODEL=sentence-transformers/all-MiniLM-L6-v2

# LLM Configuration
LLM_PROVIDER=OPENAI
OPENAI_MODEL=gpt-4o-mini

# OpenAI Configuration (REPLACE WITH YOUR ACTUAL KEY)
OPENAI_API_KEY=sk-your-actual-openai-api-key-here

# For OLLAMA (local LLM):
# OLLAMA_MODEL=llama3.1

# Optional LangSmith:
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=ls__your_langsmith_key_here
# LANGCHAIN_PROJECT=rag-mongo-demo
